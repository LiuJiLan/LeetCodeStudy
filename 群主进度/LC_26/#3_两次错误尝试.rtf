class Solution \{\
public:\
    int removeDuplicates(vector<int>& nums) \{\
        int s = 0, f = 0;\
        //s to slow, f to fast\
        \
        while (s == 0 || f < nums.size()) \{\
            if (nums[f] > nums[s]) \{\
                nums[++s] = nums[f];\
            \}\
            f++;\
        \}\
        \
        return ++s;\
        \
    \}\
\};\
